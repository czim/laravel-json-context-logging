{
    "name": "czim/laravel-json-context-logging",
    "description": "JSON Context logging for Laravel",
    "authors": [
        {
            "name": "Coen Zimmerman",
            "email": "coen.zimmerman@endeavour.nl"
        }
    ],
    "keywords": [ "laravel", "monolog", "formatting", "json" ],
    "homepage": "https://github.com/czim",
    "require": {
        "php": "^8.0",
        "czim/monolog-json-context": "^0.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^8"
    },
    "autoload": {
        "psr-4": {
            "Czim\\LaravelJsonContextLogging\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Czim\\LaravelJsonContextLogging\\Providers\\JsonContextLoggingServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
