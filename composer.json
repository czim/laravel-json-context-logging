{
    "name": "czim/laravel-json-context-logging",
    "description": "JSON Context logging for Laravel",
    "authors": [
        {
            "name": "Coen Zimmerman",
            "email": "coen@pixelindustries.com"
        }
    ],
    "keywords": [ "laravel", "monolog", "formatting", "json" ],
    "homepage": "https://github.com/czim",
    "require": {
        "czim/monolog-json-context": "^0.9"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "mockery/mockery": "0.9.*",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Czim\\LaravelJsonContextLogging\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Czim\\LaravelJsonContext\\Test\\Logging": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
